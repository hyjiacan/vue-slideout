{
  "name": "@hyjiacan/vue-slideout",
  "version": "2.1.4",
  "description": "A Slide-Out component for Vue.js 2.0",
  "main": "lib/slideout.common.js",
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "node version.js & npx vue-cli-service build --target lib --dest=lib --formats=commonjs --name slideout src/components/index.js",
    "build:docs": "npx vue-cli-service build --dest=docs",
    "release": "yarn run lint & yarn run build & yarn run build:docs",
    "lint": "npx vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyjiacan/vue-slideout.git"
  },
  "keywords": [
    "vue",
    "slide"
  ],
  "author": "hyjiacan",
  "license": "MIT",
  "files": [
    "docs",
    "lib",
    "src",
    "types",
    "README.md",
    "README.zh-CN.md"
  ],
  "typings": "types/index.d.ts",
  "bugs": {
    "url": "https://github.com/hyjiacan/vue-slideout/issues"
  },
  "homepage": "https://hyjiacan.github.io/vue-slideout/",
  "dependencies": {
    "v-scroll-lock": "^1.0.1",
    "core-js": "^2.6.5",
    "element-ui": "^2.8.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli": "^3.7.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  }
}
